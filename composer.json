{
    "name": "alexwenzel/ajax-select",
    "description": "A Laravel Nova 4 field - Ajax Select",
    "keywords": [
        "laravel",
        "nova",
        "ajax",
        "select"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.0",
        "laravel/nova": "^4.0"
    },
    "authors": [
        {
            "name": "alexwenzel",
            "email": "alexander.wenzel.berlin@gmail.com"
        },
        {
            "name": "dillingham",
            "email": "brian@dillingham.dev"
        }
    ],
    "autoload": {
        "psr-4": {
            "Alexwenzel\\AjaxSelect\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alexwenzel\\AjaxSelect\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
